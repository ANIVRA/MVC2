    <link href="~/griny/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/griny/css/colorbox.css" rel="stylesheet" />
    <link href="~/Content/ellips2.css" rel="stylesheet" />
    <link href="~/griny/css/main.css" rel="stylesheet" />
    <link href="~/griny/css/font-awesome.min.css" rel="stylesheet" />

</head>
<body>
    <div class="parallax" style="background-image: url(/griny/images/bg1.jpg);">
        <div class="text">
            <h1>My weBLOG</h1>
            <p>by ANIVRA</p>
        </div>
    </div>
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="nav-collapse pull-left">
                    <ul class="nav">
                        <li><a href="http://anivra.azurewebsites.net">Home</a></li>
                        <li>@Html.ActionLink("Blog Home", "Index", "Posts")</li>
                        @*<li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Blog Home<b class="caret"></b></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li>@Html.ActionLink("Griny", "Index", "Posts")</li>
                                    <li><a href="~/zolix-blog.html">Zolix</a></li>
                                </ul>
                            </li>*@
