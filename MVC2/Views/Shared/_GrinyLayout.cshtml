                </div>
                <div class="nav-collapse pull-right">
                    <ul class="nav">
                        <li>@Html.Partial("_LoginPartial")</li>
                    </ul>

                </div>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()

        <div class="bottom-parallax small-view" style="background-image: url(/griny/images/bg1.jpg);">
            <div class="black-bg">
                <div class="container">
                    <div class="row-fluid">
                        <div class="span9">
                        </div>
                        <div class="span3">
                            <a class="btn btn-success btn-large" href="#">Contact me</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <hr />

        <footer>
            <div class="light-bg">
                <div class="container">
                    <div class="row-fluid">
                        <div class="span3">
                            <h4>Get in touch</h4>
                            <div class="info-block">
                                <article class="clearfix">
                                    <i class="icon-globe"></i>
                                    <address>
                                        1231 SHIELDS RD<br />
                                        KERNERSVILLE, NC,<br />
                                        27284.
                                    </address>
                                </article>
                                <article class="clearfix">
                                    <i class="icon-phone"></i>
                                    <p>(336) 231-8632</p>
                                </article>
                                <article class="clearfix">
                                    <i class="icon-envelope"></i>
                                    <p><a href="araynor@coderfoundry.com">araynor@coderfoundry.com</a></p>
                                </article>
                            </div>
                        </div>
                        <div class="span3">
                            <h4>Follow us</h4>
                            <div class="social">
                                <ul class="unstyled clearfix">
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-twitter"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-facebook"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-github"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-linkedin"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-pinterest"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-google-plus"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-vk"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a target="_blank" title="" href="#">
                                            <i class="icon-rss"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        @*@{
                                Html.RenderPartial("your view", your_model);
                            }
                            @Html.RenderPartial("_RecentPosts")*@
                        <div class="span3">

                        </div>
                    </div>
                </div>
            </div>
        </footer>

        <a class="goTop goTop-link"><i class="icon-angle-up"></i></a>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <div class="copyright-block dark-bg ">
                    <div class="pull-left">
                        <p class="">Copyright &copy; 2015 by <a href="" target="_blank">anivraNET</a>. All Rights Reserved</p>
                    </div>
                    <div class="pull-right">
                        <p>Thanks <a href="http://www.unsplash.com" target="_blank">ooomf</a> for the pictures</p>
                    </div>
            </div>
